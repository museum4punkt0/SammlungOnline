  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/babel@7.0.4"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "yarn",
    "overrides": {
      "*": {
        "dependencies": {
          "react": "-",
          "react-dom": "-",
          "@material-ui/core": "-"
        },
        "devDependencies": {
          "@types/node": "+"
        },
        "peerDependencies": {
          "react": "+",
          "react-dom": "+",
          "@types/react": "+",
          "@material-ui/core": "+"
        }
      }
    }
  },
