{
    "name": "research",
    "version": "2.0.0-SNAPSHOT",
    "private": true,
    "license": "GNU GPLv3",
    "dependencies": {
        "@apollo/react-hooks": "3.1.5",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "@material-ui/styles": "^4.10.0",
        "apollo-boost": "^0.4.9",
        "apollo-client": "^2.6.4",
        "axios": "^0.21.1",
        "debounce-promise": "^3.1.2",
        "draft-js": "^0.11.7",
        "graphql": "^14.7.0",
        "graphql-tag": "^2.10.4",
        "i18next": "19.6.3",
        "i18next-browser-languagedetector": "5.0.0",
        "i18next-xhr-backend": "^3.2.2",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-cookie-consent": "^6.2.3",
        "react-dom": "^16.13.1",
        "react-hook-form": "^6.14.2",
        "react-i18next": "^11.7.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "@smb/smb-react-components-library": "git+https://gitsrv01.xailabs.dev/smb/react-component-library-2.git",
        "use-open-seadragon": "^1.0.0"
    },
    "scripts": {
        "build": "react-app-rewired build",
        "eject": "react-scripts eject",
        "generateGraphQL": "graphql-codegen --config codegen.js",
        "lint": "eslint '*/**/*.{js,ts,tsx}'",
        "lint-fix": "eslint '*/**/*.{js,ts,tsx}' --fix",
        "parseTranslations": "i18next './src/**/*.{ts,tsx}'",
        "prepare": "husky install",
        "sonar": "node xailabs_intern/sonar-project.js",
        "start": "react-app-rewired start",
        "start:dev": "REACT_APP_STAGE=dev react-app-rewired start",
        "test": "react-app-rewired test",
        "cypress:open": "cypress open",
        "cypress:update": "cypress run --env updateSnapshots=true --spec cypress/integration/regression/base_screenshot.js",
        "cypress:test": "cypress run  --spec cypress/integration/regression/base_screenshot.js"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@apollo/react-testing": "4.0.0",
        "@graphql-codegen/cli": "1.17.0",
        "@graphql-codegen/introspection": "^1.17.0",
        "@graphql-codegen/typescript": "1.17.0",
        "@graphql-codegen/typescript-operations": "1.17.0",
        "@graphql-codegen/typescript-react-apollo": "1.17.0",
        "@testing-library/cypress": "^8.0.0",
        "@testing-library/dom": "^7.20.2",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^12.0.0",
        "@testing-library/react-hooks": "^3.3.0",
        "@testing-library/user-event": "^7.2.1",
        "@types/debounce-promise": "^3.1.3",
        "@types/jest": "26.0.7",
        "@types/node": "14.0.26",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "cypress": "^8.0.0",
        "cypress-image-snapshot": "^4.0.1",
        "cypress-plugin-snapshots": "^1.4.4",
        "eslint-config-smb": "git+https://gitsrv01.xailabs.dev/smb/eslint-config-smb.git",
        "graphql-codegen-hasura-client-config": "^4.9.1",
        "graphql-codegen-hasura-core": "^4.9.1",
        "graphql-codegen-hasura-gql": "^4.9.1",
        "graphql-codegen-hasura-react": "^4.9.1",
        "graphql-codegen-hasura-typescript": "^4.9.1",
        "husky": "^7.0.1",
        "i18next-parser": "2.1.3",
        "jest-canvas-mock": "^2.3.1",
        "jest-fetch-mock": "^3.0.3",
        "lint-staged": "^11.0.1",
        "react-app-rewired": "^2.1.8",
        "react-hooks-testing-library": "^0.6.0",
        "react-test-renderer": "^16.13.1",
        "sonarqube-scanner": "^2.7.0",
        "typescript": "^3.9.9"
    },
    "bit": {
        "env": {
            "compiler": "bit.envs/compilers/react-typescript@3.1.56"
        },
        "componentsDefaultDirectory": "components/{name}",
        "packageManager": "npm",
        "overrides": {
            "*": {
                "dependencies": {
                    "react": "-",
                    "react-dom": "-",
                    "@material-ui/core": "-"
                },
                "peerDependencies": {
                    "react": "+",
                    "react-dom": "+",
                    "@types/react": "+",
                    "@material-ui/core": "+"
                }
            }
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "eslint --cache"
    }
}
